{
    "name": "doppy/nav-bundle",
    "type": "symfony-bundle",
    "description": "Bundle for providing menu's in your application.",
    "keywords": [
        "menu",
        "nav",
        "symfony"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Daan van den Berg",
            "email": "zawadi@doppy.nl"
        }
    ],
    "require": {
        "php": "^7.0",
        "symfony/framework-bundle": "^4.0",
        "twig/twig": "^2.4"
    },
    "suggest": {
        "symfony/cache": "For caching of generated Nav's",
        "symfony/stopwatch": "For timing information in the profiler (dev only)"
    },
    "autoload": {
        "psr-4": {
            "Doppy\\NavBundle\\": ""
        }
    }
}
